    <set target="opencontent/gtbg" name="server" value="gtbg"/> 
    <set target="opencontent/gtbg" name="pz:url" value="opencontent.indexdata.com:210/gutenberg"/>

  <!-- The following settings apply to all opencontent sites -->
  <set name="pz:cclmap:term" value="u=1016 t=l,r s=al 2=102"/>
